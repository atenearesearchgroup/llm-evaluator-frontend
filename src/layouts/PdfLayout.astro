---
export interface Props {
    title: string;
}

const { title } = Astro.props;
---

<!doctype html>
<html lang="en">
    <head>
        <meta charset="utf-8" />
        <title>{title }</title>
    </head>
    <body
        class="max-w-[700px] mx-auto
     bg-slate-500 bg-opacity-10
     backdrop-blur-md p-2
     "
    >
        <slot />
    </body>
</html>
